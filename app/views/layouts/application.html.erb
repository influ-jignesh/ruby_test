<!DOCTYPE html>
<html>
  <head>
    <title>Myapp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    
    <%= render 'layouts/nav' %>
    <div class="container" id="home-container">
      <%= render 'layouts/msgs' %>
      <%= yield %>
    </div>
    <%= render 'pages/footer' %>
  </body>
</html>
